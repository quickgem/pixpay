<window v-model="profile" name="profile" v-on:key_down:NETEVENT="{initHTTPCB}" v-on:key_down:cancel="{onKeyDown, Args=cancel}" style:normal:bg_color="#EFF5FF">
  <view name="inner_container1" x="0" y="0" w="320" h="38">
    <image name="image" x="10" y="0" w="36" h="38" draw_type="scale_auto" image="pixpay_icon1"/>
    <view name="view2" x="226" y="4" w="84" h="31" style:normal:bg_color="#FFFFFF" style:normal:round_radius="16">
      <label name="label3" x="21" y="0" w="43" h="28" style:normal:bg_color="#00000000" style:normal:round_radius="16" style:normal:font_size="16" style:normal:text_align_h="center" text="Logout"/>
    </view>
  </view>
  <view name="view1" x="10" y="38" w="294" h="79">
    <image name="image2" x="0" y="10" w="68" h="65" draw_type="scale_auto" image="pixpay_profile"/>
    <label name="label1" x="78" y="9" w="206" h="21" style:normal:text_align_h="left" style:normal:font_size="16" line_wrap="true" word_wrap="true" v-data:text="{studentFullName}"/>
    <label name="label2" x="78" y="30" w="80" h="18" style:normal:text_align_h="left" style:normal:font_size="14" style:normal:text_color="#A5A5A5" text="Pending Fees"/>
    <label name="label1" x="78" y="54" w="206" h="21" style:normal:text_align_h="left" style:normal:font_size="16" line_wrap="true" word_wrap="true" v-data:text="{studentTotalBillAmount}"/>
  </view>
  <view name="view" x="0" y="127" w="320" h="83">
    <button v-on:click="{handleGetNewStudentBills}" name="new_payment" x="10" y="10" w="139" h="63" style:normal:bg_color="#F79E1B" style:normal:round_radius="10">
      <label name="label" x="10" y="31" w="86" h="22" style:normal:text_color="#FFFFFF" style:normal:font_size="16" style:normal:text_align_h="left" text="New Payment"/>
      <image name="image1" x="10" y="10" w="18" h="18" draw_type="scale_auto" image="pixpay_card"/>
    </button>
    <button v-on:click="{navigateToTransaction}" name="transactions_history" x="171" y="10" w="139" h="63" style:normal:round_radius="10" style:normal:bg_color="#FFFFFF">
      <label name="label" x="10" y="30" w="119" h="22" style:normal:text_color="#000000" style:normal:font_size="16" style:normal:text_align_h="left" text="Transaction History"/>
      <image name="image1" x="10" y="10" w="18" h="18" draw_type="scale_auto" image="pixpay_trans"/>
    </button>
  </view>
  <view v-if="{isLoading}" name="loading" x="0" y="0" w="320" h="210" style:normal:bg_color="#000000cf" disg:hide="true">
    <image name="image5" x="142" y="90" w="30" h="30" draw_type="default" image="pixpay_icon2"/>
  </view>
</window>