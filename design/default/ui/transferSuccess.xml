<window v-model="transferSuccess" name="transferSuccess" v-on:key_down:NETEVENT="{initHTTPCB}" v-on:key_down:cancel="{onKeyDown, Args=cancel}">
  <view name="error" x="0" y="0" w="320" h="210" style:normal:bg_color="#F79E1B">
    <image name="image" x="130" y="32" w="60" h="60" draw_type="default" style:normal:icon="" style:normal:bg_image_draw_type="scale_auto" style:normal:font_size="16" scale_y="0.8" scale_x="0.8" style:normal:bg_image="pixpay_cancel"/>
    <label name="label" x="10" y="102" w="300" h="27" style:normal:font_size="18" style:normal:text_color="#FFFFFF" text="Sorry there was an error. Please try again"/>
    <button v-on:click="{toOnboarding}" name="cancel" x="10" y="158" w="129" h="42" style:normal:round_radius="33" style:normal:border_color="#00000000" style:normal:bg_color="#FFFFFF" style:pressed:round_radius="30" style:pressed:border="right|top|bottom" style:pressed:border_color="#00000000" style:pressed:font_size="16" style:over:border_color="#00000000" style:over:border="right|top|bottom" style:over:round_radius="30" style:normal:font_size="16" tr_text="CANCEL"/>
    <button v-on:click="{toOnboarding}" name="print" x="181" y="158" w="129" h="42" style:normal:round_radius="33" style:normal:border_color="#00000000" style:normal:bg_color="#FFFFFF" style:pressed:round_radius="30" style:pressed:border="right|top|bottom" style:pressed:border_color="#00000000" style:pressed:font_size="16" style:over:border_color="#00000000" style:over:border="right|top|bottom" style:over:round_radius="30" style:normal:font_size="16" style:normal:text_color="#F79E1B" tr_text="Try Again"/>
  </view>
  <view name="success" x="0" y="0" w="320" h="210" style:normal:bg_color="#F79E1B">
    <image name="image" x="130" y="27" w="60" h="60" draw_type="default" style:normal:icon="" style:normal:bg_image_draw_type="scale_auto" style:normal:font_size="16" scale_y="0.8" scale_x="0.8" style:normal:bg_image="Succeed"/>
    <label name="label" x="10" y="97" w="300" h="27" style:normal:font_size="18" style:normal:text_color="#FFFFFF" text="Payment successful"/>
    <button v-on:click="{toOnboarding}" name="cancel" x="10" y="158" w="129" h="42" style:normal:round_radius="33" style:normal:border_color="#00000000" style:normal:bg_color="#FFFFFF" style:pressed:round_radius="30" style:pressed:border="right|top|bottom" style:pressed:border_color="#00000000" style:pressed:font_size="16" style:over:border_color="#00000000" style:over:border="right|top|bottom" style:over:round_radius="30" style:normal:font_size="16" tr_text="CANCEL"/>
    <button v-on:click="{toOnboarding}" name="print" x="181" y="158" w="129" h="42" style:normal:round_radius="33" style:normal:border_color="#00000000" style:normal:bg_color="#FFFFFF" style:pressed:round_radius="30" style:pressed:border="right|top|bottom" style:pressed:border_color="#00000000" style:pressed:font_size="16" style:over:border_color="#00000000" style:over:border="right|top|bottom" style:over:round_radius="30" style:normal:font_size="16" style:normal:text_color="#F79E1B" tr_text="PRINT RECIEPT"/>
  </view>
</window>